{
    "name": "odolbeau/rabbit-mq-admin-toolkit",
    "description": "RabbitMQ administration toolkit",
    "license": "MIT",
    "require": {
        "php": "~5.4|~7.0",
        "symfony/console": "^2.4|^3.0",
        "symfony/yaml": "^2.4|^3.0",
        "swarrot/swarrot": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Bab\\RabbitMq": "src"
        }
    },
    "bin": ["rabbit"],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.4"
    }
}
